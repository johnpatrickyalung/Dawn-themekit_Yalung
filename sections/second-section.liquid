<section id="sec-con">
  <div class="container">
    <div class="row">
      <div class="col-12 text-center">
        <div class="con-sec">
          <h1 class="sec-title">{{section.settings.heading-title}}</h1>
        </div>
        <div class="desc-con">
          <p>{{section.settings.description}} </p>
        </div>
      </div>
    </div>
    <div class="row">
       <div class="col-12 text-center">
    	<nav class="nav">
          <a class="nav-link active" aria-current="page" href="#">Active</a>
          <a class="nav-link" href="#">Link</a>
          <a class="nav-link" href="#">Link</a>
          <a class="nav-link disabled">Disabled</a>
        </nav>
       </div>
    </div>
  </div>
</section>


